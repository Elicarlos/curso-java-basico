package com.loiane.cursojava.aula18;

public class EstudoArray {

	public static void main(String[] args) {
		double [] temperaturas = new double[365];
		temperaturas[0] = 33.7;
		temperaturas[1] = 14.7;
		temperaturas[2] = 20.7;
		temperaturas[3] = 14.7;
		temperaturas[4] = 40.7;
		temperaturas[5] = 14.7;
		temperaturas[6] = 458.7;
		temperaturas[7] = 47.7;
		
		System.out.println("A temperatura é :" + temperaturas[1]);
		System.out.println("A temperatura é :" + temperaturas[4]);
		System.out.println("A temperatura é :" + temperaturas[5]);
		System.out.println("A temperatura é :" + temperaturas[2]);
		System.out.println("A temperatura é :" + temperaturas[4]);
		System.out.println("A temperatura é :" + temperaturas[4]);

	}

}
